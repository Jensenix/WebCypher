  <style>
    /* Align styling patterns with webdev.html */
    @font-face {
      font-family: 'Rinter';
      src: url('Rinter.woff2');
    }

    :root {
      --brand-blue: #245bff;
      --brand-teal: #2ad3c9;
      --ink: #0b0c2a;
      --muted: #6c757d;
      --surface: #ffffff;
      --canvas: #f8f9fa;
      --card-radius: 18px;
    }

    html,
    body {
      height: 100%
    }

    body {
      font-family: 'Rinter', system-ui, -apple-system, Segoe UI, Roboto, Arial, sans-serif;
      background: var(--canvas);
      color: var(--ink);
      margin: 0;
    }

    /* Top bar + crumbs */
    .topbar {
      padding: 12px 0
    }

    .brand-chip {
      height: 3rem;
      width: 8rem;
      display: flex;
      align-items: center;
      gap: .6rem;
      border-radius: 50px;
      box-shadow: 0 6px 24px rgba(0, 0, 0, .06);
      overflow: hidden;
      justify-content: center;
    }

    .logo-img {
      width: 100%;
      justify-content: center
    }

    .back-link {
      color: #5b6677;
      text-decoration: none;
      font-weight: 600
    }

    .back-link:hover {
      color: #2b2f36
    }

    .crumbs {
      font-size: .9rem;
      color: #98a2b3;
      margin-bottom: 1.5rem
    }

    .crumbs a {
      color: #6c7a8a;
      text-decoration: none
    }

    .crumbs a:hover {
      color: #2b2f36;
      text-decoration: underline
    }

    /* Hero */
    .hero {
      padding: 24px 0 48px
    }

    .hero-left {
      padding-top: 24px
    }

    .hero-title {
      font-weight: 800;
      letter-spacing: -.02em
    }

    .hero-sub {
      color: #5c6676;
      font-weight: 600
    }

    .hero-copy {
      color: var(--muted);
      max-width: 54ch
    }

    @media (min-width: 992px) {
      .hero {
        padding: 16px 0 88px
      }
    }

    /* Buttons */
    .btn-gradient {
      border: 0;
      color: #fff;
      font-weight: 700;
      background: linear-gradient(180deg, var(--brand-teal), var(--brand-blue));
      border-radius: 999px;
      padding: .75rem 1.1rem;
      box-shadow: 0 8px 24px rgba(36, 91, 255, .25);
    }

    .btn-gradient:hover {
      filter: brightness(1.02)
    }

    .btn-outline {
      border: 1px solid #e2e8f0;
      background: #fff;
      color: #2b2f36;
      border-radius: 999px;
      padding: .75rem 1.1rem;
      font-weight: 700;
    }

    /* Showcase */
    .showcase {
      background: linear-gradient(180deg, var(--brand-teal), var(--brand-blue));
      height: 360px;
      border-radius: 22px;
      box-shadow: 0 18px 40px rgba(24, 32, 56, .15);
      position: relative;
      overflow: hidden;
    }

    .showcase::after {
      content: "</>";
      position: absolute;
      inset: 0;
      display: grid;
      place-items: center;
      color: rgba(255, 255, 255, .35);
      font-weight: 800;
      font-size: 160px;
      letter-spacing: -.04em;
    }

    @media (min-width: 992px) {
      .showcase {
        height: 440px
      }
    }

    /* Sections */
    .section {
      padding: 56px 0
    }

    .section h2 {
      font-weight: 800;
      letter-spacing: -.02em;
      text-align: center;
    }

    .section p.lead {
      color: var(--muted);
      text-align: center;
      max-width: 60ch;
      margin-left: auto;
      margin-right: auto;
    }

    /* Pills and chips */
    .pill {
      background: #f2f6fb;
      border: 1px solid #e9eef5;
      border-radius: 12px;
      padding: .8rem 1rem;
      display: flex;
      align-items: center;
      gap: .6rem;
      color: #364152;
    }

    .chip {
      display: inline-block;
      padding: .25rem .55rem;
      border-radius: 999px;
      border: 1px solid #e9eef5;
      background: #f8fafc;
      font-size: .8rem;
      color: #475569
    }

    .chip.time {
      background: #eefaf9;
      color: #0b8a82;
      border-color: #c8efe9
    }

    /* Cards */
    .benefit,
    .process,
    .price,
    .testi,
    .case {
      background: #fff;
      border: 1px solid #eef2f7;
      border-radius: 18px;
      box-shadow: 0 10px 28px rgba(24, 32, 56, .08);
    }

    .benefit .icon {
      width: 42px;
      height: 42px;
      border-radius: 10px;
      display: grid;
      place-items: center;
      background: linear-gradient(180deg, var(--brand-teal), var(--brand-blue));
      box-shadow: 0 6px 16px rgba(36, 91, 255, .25);
    }

    .num {
      width: 40px;
      height: 40px;
      border-radius: 10px;
      display: grid;
      place-items: center;
      color: #fff;
      font-weight: 800;
      background: linear-gradient(180deg, var(--brand-teal), var(--brand-blue));
    }

    .price.pop {
      outline: 2px solid #356dff
    }

    /* Dark tools */
    .dark {
      background: #0e1220;
      color: #e9eef5
    }

    .tool {
      background: #12172a;
      border: 1px solid #2a3045;
      border-radius: 14px;
      padding: 18px;
      text-align: center;
      color: #d8deea;
    }

    /* Results band */
    .band {
      background: linear-gradient(180deg, var(--brand-teal), var(--brand-blue));
      color: #e9f3ff
    }
    .band p.lead{color:#ffffff}

    .metric {
      background: rgba(255, 255, 255, .08);
      border: 1px solid rgba(255, 255, 255, .22);
      border-radius: 18px
    }

    /* Thumbs */
    .thumb {
      height: 200px;
      background: linear-gradient(120deg, #e6faff, #f0f4ff);
      border-bottom: 1px solid #e9eef5
    }

    /* CTA */
    .cta {background: linear-gradient(180deg, var(--brand-teal, var(--t)), var(--brand-blue, var(--b))); color:#fff; text-align:center; padding:80px 0}

    .cta .btn {
      font-weight: 700;
      background: #fff;
      color: #2b2f36;
      border-radius: 12px
    }

    .cta .form-control {
      border-radius: 12px;
      max-width: 280px
    }

    /* Badges */
    .badge-custom {
      display: inline-block;
      background: #e8f7ff;
      color: #00bcd4;
      font-weight: 700;
      font-size: .75rem;
      letter-spacing: .04em;
      padding: .45rem .8rem;
      border-radius: 999px;
    }

    /* Footer */
    footer {
      background: #0e1220;
      color: #b7c1d6;
      padding: 24px 0;
      text-align: center
    }

    /* Utilities */
    .hero+.cta {
      display: none
    }
  </style>
</head>

